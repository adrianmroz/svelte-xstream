{
  "name": "svelte-xstream",
  "version": "0.0.1",
  "description": "Utilities for working with xstream in svelte components",
  "exports": {
    "import": "./dist/index.js"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "prettier --check src/*",
    "lint:fix": "prettier --write src/*",
    "test:dev": "jest --watchAll",
    "dev": "tsc --build --watch",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrianmroz/svelte-xstream.git"
  },
  "keywords": [
    "svelte",
    "xstream"
  ],
  "author": "adrianmroz@gmail.com",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/adrianmroz/svelte-xstream/issues"
  },
  "homepage": "https://github.com/adrianmroz/svelte-xstream#readme",
  "dependencies": {
    "svelte": "3.49.0",
    "xstream": "11.13.0"
  },
  "devDependencies": {
    "husky": "4.3.0",
    "jest": "26.4.2",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "ts-jest": "26.4.1",
    "tslib": "2.0.3",
    "typescript": "4.0.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
